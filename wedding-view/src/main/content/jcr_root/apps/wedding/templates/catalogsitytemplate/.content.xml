<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
    jcr:description="Template for initial or main page of the site."
    jcr:primaryType="cq:Template"
    jcr:title="Шаблон города"
    allowedChildren="[/apps/wedding/templates/catalogproducttemplate]"
    allowedParents="[/apps/wedding/templates/catalogcategorytemplate]"
    ranking="{Long}2">
    <jcr:content
        jcr:primaryType="cq:PageContent"
        sling:resourceType="wedding/pages/catalogsitypage">
        <topnav
            jcr:primaryType="nt:unstructured"
            sling:resourceType="wedding/components/structure/topnav"/>
        <catalognavigation
                jcr:primaryType="nt:unstructured"
                sling:resourceType="wedding/components/catalog/catalogNavigation"/>
        <columns
                jcr:primaryType="nt:unstructured"
                sling:resourceType="wedding/components/structure/column-control"
                columns="[100]">
            <parsys0
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="wcm/foundation/components/parsys">
            </parsys0>
        </columns>
        <parsys
                jcr:primaryType="nt:unstructured"
                sling:resourceType="wcm/foundation/components/parsys">
        </parsys>
        <footer
                jcr:primaryType="nt:unstructured"
                sling:resourceType="wedding/components/structure/footer"/>
    </jcr:content>
</jcr:root>
