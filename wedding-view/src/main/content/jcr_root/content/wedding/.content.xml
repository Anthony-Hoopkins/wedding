<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
          xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          jcr:primaryType="nt:unstructured"
          sling:resourceType="wedding/components/pages/landing">
    <catalog
            jcr:primaryType="nt:unstructured"
            sling:resourceType="wedding/components/pages/categories">
        <category
                jcr:primaryType="nt:unstructured"
                sling:resourceType="wedding/components/pages/category">
            <partner
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="wedding/components/pages/partner"/>
        </category>
    </catalog>
    <tenders
            jcr:primaryType="nt:unstructured"
            sling:resourceType="wedding/components/pages/tenders">
        <tender
                jcr:primaryType="nt:unstructured"
                sling:resourceType="wedding/components/pages/tender"/>
    </tenders>
    <sales
            jcr:primaryType="nt:unstructured"
            sling:resourceType="wedding/components/pages/sales">
        <sale
                jcr:primaryType="nt:unstructured"
                sling:resourceType="wedding/components/pages/sale"/>
    </sales>
    <settings
            jcr:primaryType="nt:unstructured"
            sling:resourceType="wedding/components/pages/settings"/>
    <real-weddings
            jcr:primaryType="nt:unstructured"
            sling:resourceType="wedding/components/pages/weddings">
        <real-wedding
                jcr:primaryType="nt:unstructured"
                sling:resourceType="wedding/components/pages/wedding"/>
    </real-weddings>
    <user
            jcr:primaryType="nt:unstructured"
            sling:resourceType="wedding/components/pages/user"/>
    <sly-test-page
            jcr:primaryType="nt:unstructured"
            sling:resourceType="wedding/components/pages/sly-test-page"/>
</jcr:root>
